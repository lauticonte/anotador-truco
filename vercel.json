{
    "headers": [
      {
        "source": "/static/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      },
      {
        "source": "/images/(.*)", 
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      },
      {
        "source": "/favicon.ico", 
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=86400, must-revalidate" }
        ]
      }
    ]
  }
  